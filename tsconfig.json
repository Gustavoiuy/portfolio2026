{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": [
        "./src/components/*"
      ],
      "@data/*": ["./src/data/*"],
      "@interface/*": ["./src/interface/*"],
      "@projects/*": ["./src/assets/images/projects/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}